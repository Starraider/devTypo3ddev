{
  "name": "typo3/cms-base-distribution",
  "description": "TYPO3 CMS Base Distribution",
  "license": "GPL-2.0-or-later",
  "repositories": [
    {
      "type": "path",
      "url": "./packages/*"
    }
  ],
  "config": {
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true
  },
  "require": {
    "helhum/typo3-console": "^6.0",
    "typo3/cms-about": "^10.4",
    "typo3/cms-backend": "^10.4",
    "typo3/cms-belog": "^10.4",
    "typo3/cms-beuser": "^10.4",
    "typo3/cms-core": "^10.4",
    "typo3/cms-dashboard": "^10.4",
    "typo3/cms-extbase": "^10.4",
    "typo3/cms-extensionmanager": "^10.4",
    "typo3/cms-felogin": "^10.4",
    "typo3/cms-filelist": "^10.4",
    "typo3/cms-filemetadata": "^10.4",
    "typo3/cms-fluid": "^10.4",
    "typo3/cms-fluid-styled-content": "^10.4",
    "typo3/cms-form": "^10.4",
    "typo3/cms-frontend": "^10.4",
    "typo3/cms-impexp": "^10.4",
    "typo3/cms-info": "^10.4",
    "typo3/cms-install": "^10.4",
    "typo3/cms-introduction": "^4.2",
    "typo3/cms-lowlevel": "^10.4",
    "typo3/cms-recordlist": "^10.4",
    "typo3/cms-recycler": "^10.4",
    "typo3/cms-redirects": "^10.4",
    "typo3/cms-reports": "^10.4",
    "typo3/cms-rte-ckeditor": "^10.4",
    "typo3/cms-scheduler": "^10.4",
    "typo3/cms-seo": "^10.4",
    "typo3/cms-setup": "^10.4",
    "typo3/cms-sys-note": "^10.4",
    "typo3/cms-t3editor": "^10.4",
    "typo3/cms-tstemplate": "^10.4",
    "typo3/cms-viewpage": "^10.4",
    "typo3/cms-workspaces": "^10.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "helmich/typo3-typoscript-lint": "^2.1",
    "php-parallel-lint/php-parallel-lint": "^1.0",
    "phploc/phploc": "^5.0",
    "phpmd/phpmd": "^2.8",
    "phpstan/phpstan": "^0.12.11",
    "squizlabs/php_codesniffer": "^3.5",
    "typo3/cms-adminpanel": "^10.4"
  },
  "scripts": {
    "typo3-cms-scripts": [
      "typo3cms install:fixfolderstructure",
      "typo3cms install:generatepackagestates"
    ],
    "post-autoload-dump": [
      "@typo3-cms-scripts"
    ]
  }
}
